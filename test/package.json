{
  "dependencies": {
    "@types/jest": "^21.1.4",
    "@types/lodash": "^4.14.74",
    "@types/node": "^8.0.28",
    "@types/opn": "^3.0.28",
    "@types/reflect-metadata": "^0.0.5",
    "@types/selenium-webdriver": "^3.0.7",
    "inquirer-promise": "^1.0.0",
    "inquirer-shortcuts": "^1.1.0",
    "jest": "^21.2.1",
    "lodash": "^4.17.4",
    "opn": "^5.1.0",
    "selenium-webdriver": "^3.5.0",
    "snappit-visual-regression": "^0.6.4",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2"
  },
  "scripts": {
    "build": "tsc",
    "build:visreg": "tsc bin/visreg.ts bin/visreg.config.ts --outdir built/bin --lib es6 && tsc",
    "clean": "npm run clean:js && npm run clean:screenshots",
    "clean:screenshots": "rm -rf screenshots",
    "clean:js": "find built \\( ! -path 'built/unit/__snapshots__/*' \\) -delete",
    "lint": "tslint index.ts",
    "test": "npm run build && jest",
    "test:unit": "npm run build && jest --config unit/jest.config.json",
    "previsreg": "[ -f ./bin/visreg.config.ts ] || cp bin/visreg{.example,}.config.ts",
    "visreg": "npm run clean:js && npm run build:visreg && node ./built/bin/util.js && node ./built/bin/visreg.js",
    "webdriver": "npm run webdriver:update && npm run webdriver:start",
    "webdriver:update": "scripts/webdriver-update",
    "webdriver:start": "scripts/webdriver-start"
  },
  "jest": {
    "testRegex": "built/(chrome|firefox).js",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js"
    ]
  }
}
